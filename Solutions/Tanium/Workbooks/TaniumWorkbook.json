            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "TaniumComplyCompliance_CL \n|where  Rule_ID_s <> \"\" and Status_Category_s has \"fail\"\n|summarize Count=count() by RuleID=Rule_ID_s\n|top 10 by Count",
